<launch>
   <node name="hokuyo" pkg="hokuyo_node" type="hokuyo_node">
      <param name="port" value="/dev/ttyACM0"/>
   </node>

   <node name="rosserial" pkg="rosserial_python" type="serial_node.py">
      <param name="port" value="/dev/ttyS1"/>
      <param name="baud" value="115200"/>
   </node>

   <node name="transform" pkg="tf" type="static_transform_publisher" args="0.26 0 0.1 0 0 0 base_link laser 100.0" />

   <node name="odom" pkg="dagny_odom" type="dagny_odom"/>
</launch>
